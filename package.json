{
  "name": "university-training-programs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "env $(grep -v '^#' .env) nuxt build",
    "start": "env $(grep -v '^#' .env) nuxt start  --hostname 0.0.0.0 --port 3010",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "prepare": "husky install",
    "deploy:fe:clean": "git fetch origin --prune 'refs/tags/*:refs/tags/*' && git push origin --delete $(git tag -l 'deploy-be*') || true && git fetch origin --prune 'refs/tags/*:refs/tags/*'",
    "deploy:fe": "yarn deploy:fe:clean && TAG=`echo deploy-fe$(date +%Y%m%d-%H%M%S)` && git tag $TAG && git push origin $TAG"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-checkbox": "^0.27.0",
    "@material/mwc-dialog": "^0.27.0",
    "@material/mwc-icon": "^0.27.0",
    "@material/mwc-select": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/toast": "^3.3.1",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.19.3",
    "ky": "^0.30.0",
    "ky-universal": "^0.10.1",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-ads-table-tree": "^2.4.2",
    "vue-multiselect": "^2.1.6",
    "vue-multiselect-listbox": "^0.2.6",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuelidate": "^0.7.7",
    "vuex-pathify": "^1.2.4",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "sass": "^1.55.0",
    "sass-loader": "10.1.1"
  }
}
